{
    "Serilog": {
        "WriteTo": [
            {
                "Name": "LiterateConsole",
                "Args": {
                    "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} | {Level:u3} | {CorrelationId} | {MachineName}({ThreadId})] {Message}{NewLine}{RequestContext}{NewLine}{Exception}"
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "%TEMP%\\Logs\\docit.txt",
                    "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} | {Level:u3} | {CorrelationId} | {MachineName}({ThreadId})] {Message}{NewLine}{RequestContext}{NewLine}{Exception}"
                }
            }
        ]
    },

    "Redis": {
        "Host": "localhost",
        "Port": "6379",
        "Name":  "docit-redis"
    }
}